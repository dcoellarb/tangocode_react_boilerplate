{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "elint": "eslint ./src/**/** - ext .jsx - ext .js — cache || true",
    "elint:table": "eslint . — ext .js — ext .jsx — cache — format table || true",
    "elint:summary": "eslint . — ext .js — ext .jsx — cache — format 'node_modules/eslint-friendly-formatter' || true",
    "elintjsx": "eslint src/Components/**/*.jsx",
    "test": "mocha --timeout 15000 --compilers js:babel-register",
    "watch:test": "npm t -- -w",
    "coverage": "nyc --reporter=lcov npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Tangocode",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "image-webpack-loader": "^1.8.0",
    "mocha": "^2.5.3",
    "mocha-jsdom": "^1.1.0",
    "nyc": "^6.4.4",
    "react-hot-loader": "^1.3.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.4.1",
    "material-ui": "^0.15.0",
    "node-file-loader": "^0.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-dropzone": "^3.5.1",
    "react-emoji": "^0.4.4",
    "react-emoji-picker": "^1.0.12",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-swipeable": "^3.5.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2"
  },
  "main": "server.js"
}
